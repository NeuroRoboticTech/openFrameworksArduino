# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := openFrameworksArduino ArduinoTest

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

openFrameworksArduino: 
	@echo "==== Building openFrameworksArduino ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f openFrameworksArduino.make

ArduinoTest: openFrameworksArduino
	@echo "==== Building ArduinoTest ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f ArduinoTest.make

clean:
	@${MAKE} --no-print-directory -C . -f openFrameworksArduino.make clean
	@${MAKE} --no-print-directory -C . -f ArduinoTest.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   openFrameworksArduino"
	@echo "   ArduinoTest"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
